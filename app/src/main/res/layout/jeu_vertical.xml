<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".jeu">

    <RelativeLayout
        android:id="@+id/pouf"
        android:layout_width="86dp"
        android:layout_height="95dp"
        android:layout_marginStart="250dp"
        android:layout_marginTop="312dp"
        android:layout_marginEnd="75dp"
        android:layout_marginBottom="324dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/notif_pouffr"
            android:layout_width="43dp"
            android:layout_height="44dp"
            android:adjustViewBounds="false"
            android:contentDescription="@string/notification"
            app:srcCompat="@drawable/notification"
            android:visibility="invisible"/>

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/tableau"
        android:layout_width="62dp"
        android:layout_height="148dp"
        android:layout_marginStart="340dp"
        android:layout_marginTop="295dp"
        android:layout_marginEnd="9dp"
        android:layout_marginBottom="288dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/notif_tableaufr"
            android:layout_width="43dp"
            android:layout_height="44dp"
            android:adjustViewBounds="false"
            android:contentDescription="@string/notification"
            app:srcCompat="@drawable/notification"
            android:visibility="invisible"/>

    </RelativeLayout>

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="459dp"
        android:layout_height="335dp"
        android:layout_marginTop="212dp"
        android:layout_marginBottom="184dp"
        android:contentDescription="@string/imagejeu"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/salledejeu" />


    <RelativeLayout
        android:id="@+id/guitare"
        android:layout_width="84dp"
        android:layout_height="77dp"
        android:layout_marginStart="260dp"
        android:layout_marginTop="407dp"
        android:layout_marginEnd="67dp"
        android:layout_marginBottom="247dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:visibility="visible">


        <ImageView
            android:id="@+id/notif_guitarefr"
            android:layout_width="43dp"
            android:layout_height="44dp"
            android:adjustViewBounds="false"
            android:contentDescription="@string/notification"
            app:srcCompat="@drawable/notification"
            android:visibility="invisible"/>

    </RelativeLayout>


<RelativeLayout
        android:id="@+id/batterie"
        android:layout_width="180dp"
        android:layout_height="110dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="314dp"
        android:layout_marginEnd="215dp"
        android:layout_marginBottom="307dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    <ImageView
        android:id="@+id/notif_batteriefr"
        android:layout_width="43dp"
        android:layout_height="44dp"
        android:adjustViewBounds="false"
        android:contentDescription="@string/notification"
        app:srcCompat="@drawable/notification"
        android:visibility="invisible"/>


    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/xylophone"
        android:layout_width="92dp"
        android:layout_height="84dp"
        android:layout_marginStart="168dp"
        android:layout_marginTop="443dp"
        android:layout_marginEnd="151dp"
        android:layout_marginBottom="204dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" >

        <ImageView
            android:id="@+id/notif_xylophonefr"
            android:layout_width="43dp"
            android:layout_height="44dp"
            android:adjustViewBounds="false"
            android:contentDescription="@string/notification"
            app:srcCompat="@drawable/notification"
            android:visibility="invisible"/>
    </RelativeLayout>




</android.support.constraint.ConstraintLayout>