<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Chambre">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="459dp"
        android:layout_height="686dp"
        android:layout_marginBottom="45dp"
        android:contentDescription="@string/imagechambre"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/chambre1" />

    <RelativeLayout
        android:id="@+id/lit"
        android:layout_width="199dp"
        android:layout_height="74dp"
        android:layout_marginStart="140dp"
        android:layout_marginTop="307dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/notif_lit"
            android:layout_width="43dp"
            android:layout_height="44dp"
            android:adjustViewBounds="false"
            android:contentDescription="@string/notification"
            app:srcCompat="@drawable/notification"
            android:visibility="invisible"/>
    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/fenetre2"
        android:layout_width="90dp"
        android:layout_height="104dp"
        android:layout_marginStart="209dp"
        android:layout_marginTop="203dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <ImageView
            android:id="@+id/notif_fen_2"
            android:layout_width="43dp"
            android:layout_height="44dp"
            android:adjustViewBounds="false"
            android:contentDescription="@string/notification"
            app:srcCompat="@drawable/notification"
            android:visibility="gone"/>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/tableau"
        android:layout_width="79dp"
        android:layout_height="100dp"
        android:layout_marginStart="347dp"
        android:layout_marginTop="315dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <ImageView
            android:id="@+id/notif_tab"
            android:layout_width="43dp"
            android:layout_height="44dp"
            android:adjustViewBounds="false"
            android:contentDescription="@string/notification"
            app:srcCompat="@drawable/notification"
            android:visibility="gone"/>
    </RelativeLayout>
</android.support.constraint.ConstraintLayout>