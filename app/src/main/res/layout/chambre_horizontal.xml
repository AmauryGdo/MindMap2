<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Chambre">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="688dp"
        android:layout_height="843dp"
        android:layout_marginStart="27dp"
        android:layout_marginEnd="6dp"
        android:layout_marginBottom="45dp"
        android:contentDescription="@string/imagechambre"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.627"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.41"
        app:srcCompat="@drawable/chambre1" />

    <RelativeLayout
        android:id="@+id/lit"
        android:layout_width="276dp"
        android:layout_height="87dp"
        android:layout_marginStart="296dp"
        android:layout_marginTop="180dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/notif_lit"
            android:layout_width="43dp"
            android:layout_height="44dp"
            android:adjustViewBounds="false"
            android:contentDescription="@string/notification"
            android:visibility="gone"
            app:srcCompat="@drawable/notification"
            tools:ignore="DuplicateIds" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/fenetre2"
        android:layout_width="132dp"
        android:layout_height="137dp"
        android:layout_marginStart="384dp"
        android:layout_marginTop="24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/notif_fen_2"
            android:layout_width="43dp"
            android:layout_height="44dp"
            android:adjustViewBounds="false"
            android:contentDescription="@string/notification"
            app:srcCompat="@drawable/notification"
            android:visibility="invisible"/>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/tableau"
        android:layout_width="81dp"
        android:layout_height="222dp"
        android:layout_marginStart="600dp"
        android:layout_marginTop="136dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/notif_tab"
            android:layout_width="43dp"
            android:layout_height="44dp"
            android:adjustViewBounds="false"
            android:contentDescription="@string/notification"
            app:srcCompat="@drawable/notification"
            android:visibility="invisible"
            tools:ignore="DuplicateIds" />
    </RelativeLayout>


</android.support.constraint.ConstraintLayout>